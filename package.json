{
  "name": "@aics/allencell-nav-bar",
  "version": "1.0.3",
  "description": "Nav bar for allen cell subdomains.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.js",
  "types": "type-declarations",
  "scripts": {
    "build": "npm-run-all --print-label clean --parallel transpile:commonjs transpile:es generateTypes",
    "prepublish": "npm run build",
    "clean": "rimraf es lib type-declarations",
    "generateTypes": "tsc -p tsconfig.base.json --emitDeclarationOnly",
    "lint": "eslint src/**/*.ts",
    "postversion": "git push origin `git rev-parse --abbrev-ref HEAD` && git push origin --tags `git rev-parse --abbrev-ref HEAD`",
    "transpile:commonjs": "TS_NODE_PROJECT=tsconfig.commonjs.json BABEL_ENV=commonjs babel src --out-dir lib --extensions .ts,.tsx --ignore src/**/*.test.ts",
    "transpile:es": "TS_NODE_PROJECT=tsconfig.es.json BABEL_ENV=es babel src --out-dir es --extensions .ts,.tsx --ignore src/**/*.test.ts",
    "typeCheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "es",
    "lib",
    "dist",
    "style",
    "type-declarations",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "~7.2",
    "@babel/core": "~7.2",
    "@babel/preset-env": "~7.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "~7.1",
    "@types/chai": "~4.1",
    "@types/classnames": "^2.2.8",
    "@types/mocha": "~5.2",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "~1.1",
    "@typescript-eslint/parser": "~1.1",
    "babel-loader": "~8.0",
    "chai": "~4.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "mocha": "~5.2",
    "npm-run-all": "^4.1.5",
    "nyc": "~13.1",
    "postcss-cli": "^6.1.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "~2.6",
    "ts-node": "~8.0",
    "typescript": "~3.2",
    "webpack": "~4.29",
    "webpack-cli": "~3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
